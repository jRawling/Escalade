{
	"webroot": "wwwroot",
	"version": "1.0.0-*",
	"dependencies": {
		"Microsoft.AspNet.Server.IIS": "1.0.0-*",
		"Microsoft.AspNet.Identity": "3.0.0-*",
		"Microsoft.AspNet.Mvc": "6.0.0-*",
